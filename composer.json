{
    "name": "helidalto/laravel6-full-calendar",
    "description": "Laravel 6 helper for FullCalendar.io. Fork of maddhatter/laravel-fullcalendar.",
    "authors": [
        {
            "name": "Helidalto",
            "email": "helidalto@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0",
        "laravel/helpers": "^1.1"
    },
    "license": "MIT",
    "homepage": "https://github.com/Helidalto/fullcallendar-l6",
    "support": {
        "email": "helidalto@gmail.com",
        "issues": "https://github.com/Helidalto/fullcallendar-l6/issues",
        "source": "https://github.com/Helidalto/fullcallendar-l6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
